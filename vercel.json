{
  "version": 2,
  "builds": [
    {
      "src": "views/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/views/$1"
    },
    {
      "src": "/",
      "dest": "/views/index.html"
    }
  ]
}
